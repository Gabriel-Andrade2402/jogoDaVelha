package game.computer;

import game.board.Board;

public class ComputerMedium extends Computer{

	@Override
	public String Play() {
		// TODO Auto-generated method stub
		return "";
	}
	public String caseDefence() {
		// Check Lines
		for (int line = 0; line < 3; line++) {
			if (board[line][0] == "X" && board[line][1] == "X" && board[line][2] == "X") {
				if (mark == "X") {
					return "Vitória do Jogador";
				} else {
					return "Vitória do Computador";
				}
			}
		}
		return "";
	}
}
